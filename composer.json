{
    "name": "houjit021/thinkphp-jump",
    "description": "适用于thinkphp6.0的跳转扩展,支持复制二开",
    "keywords": ["thinkphp", "think-jump", "success","error","result","redirect"],
    "license": "Apache-2.0",
    "type": "think-extend",
    "authors": [
        {
            "name": "houjit021",
            "email": "amos@houjit.com"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "topthink/framework": "^6.0",
        "topthink/think-view": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "houjit021\\think\\": "src/"
        }
    },
    "extra": {
        "think": {
            "config": {
                "jump": "src/config/jump.php"
            }
        }
    }
}
